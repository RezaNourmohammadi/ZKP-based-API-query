//basic hash pre-image
import "hashes/sha256/512bitPacked" as sha256packed;

//A field only has 254 bits. Hashes are broken down to two 128-bit fields, inputs in 4 of those
def main(private field a, private field b, private field c, private field d, field h0, field h1) {
    field[2] h = sha256packed([a, b, c, d]);
    assert(h[0] == h0);
    assert(h[1] == h1);
    return;
}

