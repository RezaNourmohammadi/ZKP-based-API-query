//Hash generator - generates 100 hashes to play with
import "hashes/sha256/512bitPacked" as sha256packed;

def main(private field[100][4] pre) -> field[100][2] {
    field[100][2] mut result = [[0;2];100];
    for u32 i in 0..100 {
        result[i] = sha256packed(pre[i]);
    }
    return result;
}


//Hash pre-image verify x100
import "hashes/sha256/512bitPacked" as sha256packed;

def main(private field[100][4] pre, field[100][2] h) {
    for u32 i in 0..100 {
        field[2] hashed = sha256packed(pre[i]);
        assert(hashed[0] == h[i][0]);
        assert(hashed[1] == h[i][1]);
    }
    return;
}
