
// Basic age check

def main(private field age) {
    assert(age > 18);
    return;
}

// Age check x1000
def main(private u32[1000] ages, u32 total) {
    u32 mut count = 0;
    for u32 i in 0..1000{
        count = count + if ages[i] >= 18 {1} else {0};
    }
    assert (count == total);
    return;
}
