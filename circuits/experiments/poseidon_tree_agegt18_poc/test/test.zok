// from "snark/gm17" import main as verify, Proof, VerificationKey;

import "../lib/poseidon/poseidon" as poseidon;

//tree verification for 10-level tree: max 1024 element capacty
// The sequence of elements: walletAddr, name, age, email id
def main(field[2] elements) -> field[3]{
    log("elements: {}", elements);
    return [
        poseidon(elements[0..1]),
        poseidon([elements[1]]),
        poseidon([elements[0], elements[1]])
        ];
}


